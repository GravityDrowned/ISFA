        <theory name="A001286">
          <assertion class="***** TTTT *****">
      <CMP> M4225 N1766</CMP>
    </assertion><omtext class="starts-with">
      <CMP> 1,6,36,240,1800,15120,141120,1451520,16329600,199584000,2634508800,</CMP>
