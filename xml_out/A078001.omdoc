    </omdoc:p><omtext class="mathematica">
      <CMP> CoefficientList[Series[(1-x)/(1-2x+x^2+x^3),{x,0,50}],x] (* or *) LinearRecurrence[{2,-1,-1},{1,1,1},50] (* _Harvey P. Dale_, Nov 03 2013 *)</CMP>
    </omtext><omtext class="program">
      <CMP> (Python)</CMP>
    </omtext><omtext class="program">
      <CMP> a = [1]*1000</CMP>
    </omtext><omtext class="program">
      <CMP> for n in range(55):</CMP>
    </omtext><omtext class="program">
      <CMP> .    print a[n],</CMP>
    </omtext><omtext class="program">
      <CMP> .    sum=0</CMP>
    </omtext><omtext class="program">
      <CMP> .    for k in range(n-1):</CMP>
    </omtext><omtext class="program">
      <CMP> .    .    sum+=a[k]</CMP>
    </omtext><omtext class="program">
      <CMP> .    a[n+1] = a[n]-sum</CMP>
    </omtext><omtext class="program">
      <CMP> # from _Alex Ratushnyak_, May 03 2012</CMP>
