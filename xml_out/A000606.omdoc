    </omtext><omtext class="mathematica">
      <CMP> nn = 50; t = Table[0, {nn}]; Do[d = x^2 + y^2 + z^2; If[0 &lt; d &lt;= nn, t[[d]]++], {x, 0, nn}, {y, 0, nn}, {z, 0, nn}]; Accumulate[Join[{1}, t]] (* _T. D. Noe_, Apr 01 2013 *)</CMP>
    </omtext><omtext class="program">
      <CMP> (Python)</CMP>
    </omtext><omtext class="program">
      <CMP> for n in range(99):  # from _Alex Ratushnyak_, Apr 01 2013</CMP>
    </omtext><omtext class="program">
      <CMP>   k = 0</CMP>
    </omtext><omtext class="program">
      <CMP>   for x in range(99):</CMP>
    </omtext><omtext class="program">
      <CMP>     s = x*x</CMP>
    </omtext><omtext class="program">
      <CMP>     if s&gt;n: break</CMP>
    </omtext><omtext class="program">
      <CMP>     for y in range(99):</CMP>
    </omtext><omtext class="program">
      <CMP>         sy = s + y*y</CMP>
    </omtext><omtext class="program">
      <CMP>         if sy&gt;n: break</CMP>
    </omtext><omtext class="program">
      <CMP>         for z in range(99):</CMP>
    </omtext><omtext class="program">
      <CMP>             sz = sy + z*z</CMP>
    </omtext><omtext class="program">
      <CMP>             if sz&gt;n: break</CMP>
    </omtext><omtext class="program">
      <CMP>             k+=1</CMP>
    </omtext><omtext class="program">
      <CMP>   print str(k)+',',</CMP>
