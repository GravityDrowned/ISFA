      </omdoc:p><omtext class="mathematica">
      <CMP> Table[Sum[SquaresR[3, k], {k, 0, n^2}], {n, 0, 37}]</CMP>
    </omtext><omtext class="program">
      <CMP> (C)</CMP>
    </omtext><omtext class="program">
      <CMP> int A000605(int i)</CMP>
    </omtext><omtext class="program">
      <CMP> {</CMP>
    </omtext><omtext class="program">
      <CMP>     const int ring = i*i;</CMP>
    </omtext><omtext class="program">
      <CMP>     int result = 0;</CMP>
    </omtext><omtext class="program">
      <CMP>     for (int a = -i; a &lt;= i; a++)</CMP>
    </omtext><omtext class="program">
      <CMP>         for (int b = -i; b &lt;= i; b++)</CMP>
    </omtext><omtext class="program">
      <CMP>             for (int c = -i; c &lt;= i; c++)</CMP>
    </omtext><omtext class="program">
      <CMP>                 if ( ring &gt;= a*a+b*b+c*c )  result++;</CMP>
    </omtext><omtext class="program">
      <CMP>     return result;</CMP>
    </omtext><omtext class="program">
      <CMP> } /* _Oskar Wieland_, Apr 08 2013 */</CMP>
