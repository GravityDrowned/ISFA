        <theory name="A000740">
          <assertion class="***** TTTT *****">
      <CMP> M2582 N1021</CMP>
    </assertion><omtext class="starts-with">
      <CMP> 1,1,3,6,15,27,63,120,252,495,1023,2010,4095,8127,16365,32640,65535,</CMP>
