    </assertion><omtext class="mathematica">
      <CMP> An[d_] := Table[If[n + m - 1 &gt; d, 0, 1], {n, 1, d}, {m, 1, d}]; a = Join[{{1}}, Table[CoefficientList[CharacteristicPolynomial[An[d],x], x], {d, 1, 20}]]; w = Join[{{{1}}}, Table[Sum[MatrixPower[a[[n]][[m + 1]]*An[n], m - 1], {m, 0, Length[a[[n]]] - 1}], {n, 2, 10}]]; Join[{{1}}, Table[CoefficientList[CharacteristicPolynomial[w[[d]], x], x], {d, 1, Length[w]}]]; Flatten[%]</CMP>
