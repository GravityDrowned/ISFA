      <CMP> A000204 := proc(n) option remember; if n &lt;=2 then 2*n-1; else A000204(n-1)+A000204(n-2); fi; end;</CMP>
    </omtext><omtext class="maple">
      <CMP> with(combinat): A000204 := n-&gt;fibonacci(n+1)+fibonacci(n-1);</CMP>
    </omtext><omtext class="maple">
      <CMP> L[1]:=1: L[2]:=3: for n from 3 to 34 do L[n]:=L[n-1]+L[n-2] od:seq(L[n],n=1..34);</CMP>
    </omtext><omtext class="maple">
      <CMP> a:=n-&gt;2*fibonacci(n-1)+fibonacci(n): seq(a(n), n=1..34); # _Zerinvary Lajos_, Oct 05 2007</CMP>
    </omtext><omtext class="maple">
