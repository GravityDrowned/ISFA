      <CMP> with(numtheory); T := proc(n::posint) local x, y, S; S:=divisors(n); x:=nops( select(z-&gt;type(z,odd),S) ); y:=nops( select(z-&gt;type(z,even),S) ); return [x,y] end; RF:=[]: N:=12^6/2: CNT:=12^4: for w to 1 do for k from 1 to N do n:=2*k; if k mod CNT = 0 then print((N-k)/CNT) fi; r:=T(n)[1]; s:=T(n)[2]; t:=r+s; if type(s,even) and type(r,odd) and andmap(z -&gt; n mod z = 0, [r,s,t]) then RF:=[op(RF),n]; print(n,r,s,t); fi; od od; RF;</CMP>
