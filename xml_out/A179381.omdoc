    </assertion><assertion class="example">
      <CMP> 1 1</CMP>
    </assertion><assertion class="example">
      <CMP> 2 1 1</CMP>
    </assertion><assertion class="example">
      <CMP> 5 2 1 1 1</CMP>
    </assertion><assertion class="example">
      <CMP> 14 5 2 2 1 1 1</CMP>
    </assertion><assertion class="example">
      <CMP> so</CMP>
    </assertion><assertion class="example">
      <CMP> a(n) begins 1 2 4 10 26 ...</CMP>
    </assertion><omtext class="program">
      <CMP> (PARI)</CMP>
    </omtext><omtext class="program">
      <CMP> N = 66;  x = 'x +O('x^N);</CMP>
    </omtext><omtext class="program">
      <CMP> C(n) = binomial(2*n,n)/(n+1);</CMP>
    </omtext><omtext class="program">
      <CMP> gf = -1 + 1/prod(n=1, N, 1 - C(n-1)*x^n );</CMP>
    </omtext><omtext class="program">
      <CMP> Vec(gf)</CMP>
    </omtext><omtext class="program">
      <CMP> \\ _Joerg Arndt_, Aug 18 2014</CMP>
    </omtext><omtext class="program">
      <CMP> (Maxima)</CMP>
    </omtext><omtext class="program">
      <CMP> C(n):=  1/(n+1)*binomial(2*n,n);</CMP>
    </omtext><omtext class="program">
      <CMP> s(m,n):=if m&gt;n then 0 else if n=m then C(n-1) else sum(C(k-1)*s(k,n-k),k,m,ceiling(n/2))+C(n-1);</CMP>
    </omtext><omtext class="program">
      <CMP> makelist(s(1,n),n,1,27);  /* _Vladimir Kruchinin_, Sep 06 2014 */</CMP>
