      <CMP> foundQ[n_, m_] := (idn = IntegerDigits[n]; ln = Length[idn]; rdm = Most[ Flatten[ RealDigits[1/m, 10, lm = 2* Length[ Flatten[ RealDigits[1/m]]]]]]; found = False; Do[If[idn === Take[rdm, {k, k+ln-1}], found = True; Break[]], {k, 1, lm-ln+1}]; found); a[n_] := (m = 2; While[ True, If[foundQ[n, m], Break[], m++]]; m); a[n_ /; n &gt; 1 &amp;&amp; IntegerQ[ Log[10, n]]] := 10; a[n_ /; n &gt; 1 &amp;&amp; Union[ IntegerDigits[n]] == {1}] := 9; Table[a[n], {n, 1, 76}] (* _Jean-Fran√ßois Alcover_, Oct 24 2011 *)</CMP>
