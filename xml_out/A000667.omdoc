      <CMP> With[{nn=30},CoefficientList[Series[Exp[x](Tan[x]+Sec[x]),{x,0,nn}], x]Range[0,nn]!] (* _Harvey P. Dale_, Nov 28 2011 *)</CMP>
    </omtext><omtext class="mathematica">
      <CMP> Clear[nn, A1, A2, A3, A4, A5, A6, A7, A8, A9]</CMP>
    </omtext><omtext class="mathematica">
      <CMP> nn = 22;</CMP>
    </omtext><omtext class="mathematica">
      <CMP> Clear[t, n, k];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> t[n_, 1] = 1;</CMP>
    </omtext><omtext class="mathematica">
      <CMP> t[n_, k_] := t[n, k] = If[n &gt;= k, t[n - 1, k - 1] + t[n - 1, k], 0];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A1 = Table[Table[t[n, k], {k, 1, nn}], {n, 1, nn}];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A1];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> Clear[t, n, k];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> t[n_, 1] = If[Or[Mod[n, 4] == 1, Mod[n, 4] == 0], 1, -1];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> t[n_, k_] := t[n, k] = If[n &gt;= k, t[n - 1, k - 1], 0];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A2 = Table[Table[t[n, k], {k, 1, nn}], {n, 1, nn}];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A2];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> Clear[t, n, k];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> t[n_, k_] :=</CMP>
    </omtext><omtext class="mathematica">
      <CMP>   t[n, k] = If[n &gt;= k, If[n == k, 1, If[Mod[k, 2] == 1, 0, 1]], 0];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A3 = Table[Table[t[n, k], {k, 1, nn}], {n, 1, nn}];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A3];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> Clear[t, n, k];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> t[n_, k_] :=</CMP>
    </omtext><omtext class="mathematica">
      <CMP>   t[n, k] = If[n &gt;= k, If[n == k, 1, If[Mod[k, 2] == 1, 1, 0]], 0];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A4 = Table[Table[t[n, k], {k, 1, nn}], {n, 1, nn}];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A4];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A5 = A1*A2*A3;</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A5];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A6 = A1*A2*A4;</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A6];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A7 = Inverse[A5];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A7];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A8 = Inverse[A6];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A8];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> A9 = A7 + A8 - IdentityMatrix[nn];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> MatrixForm[A9];</CMP>
    </omtext><omtext class="mathematica">
      <CMP> Total[Transpose[A9]] (* _Mats Granvik_, Nov 25 2013 *)</CMP>
    </omtext><omtext class="program">
      <CMP> (Sage) # Algorithm of L. Seidel (1877)</CMP>
    </omtext><omtext class="program">
      <CMP> def A000667_list(n) :</CMP>
    </omtext><omtext class="program">
      <CMP>     R = []; A = {-1:0, 0:0}</CMP>
    </omtext><omtext class="program">
      <CMP>     k = 0; e = 1</CMP>
    </omtext><omtext class="program">
      <CMP>     for i in range(n) :</CMP>
    </omtext><omtext class="program">
      <CMP>         Am = 1</CMP>
    </omtext><omtext class="program">
      <CMP>         A[k + e] = 0</CMP>
    </omtext><omtext class="program">
      <CMP>         e = -e</CMP>
    </omtext><omtext class="program">
      <CMP>         for j in (0..i) :</CMP>
    </omtext><omtext class="program">
      <CMP>             Am += A[k]</CMP>
    </omtext><omtext class="program">
      <CMP>             A[k] = Am</CMP>
    </omtext><omtext class="program">
      <CMP>             k += e</CMP>
    </omtext><omtext class="program">
      <CMP>         # print [A[z] for z in (-i//2..i//2)]</CMP>
    </omtext><omtext class="program">
      <CMP>         R.append(A[e*i//2])</CMP>
    </omtext><omtext class="program">
      <CMP>     return R</CMP>
    </omtext><omtext class="program">
      <CMP> A000667_list(10)  # _Peter Luschny_, Jun 02 2012</CMP>
    </omtext><omtext class="program">
      <CMP> (Haskell)</CMP>
    </omtext><omtext class="program">
      <CMP> a000667 n = if x == 1 then last xs else x</CMP>
    </omtext><omtext class="program">
      <CMP>             where xs@(x:_) = a227862_row n</CMP>
    </omtext><omtext class="program">
      <CMP> -- _Reinhard Zumkeller_, Nov 01 2013</CMP>
