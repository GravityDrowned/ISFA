    </omtext><omtext class="maple">
      <CMP> N:= 1000; # to get all terms &lt;= N</CMP>
    </omtext><omtext class="maple">
      <CMP> Primes:= select(isprime,[seq(2*n+1,n=1..floor((N-1)/2))]):</CMP>
    </omtext><omtext class="maple">
      <CMP> filter:= proc(p) local S;</CMP>
    </omtext><omtext class="maple">
      <CMP>   S:= remove(hastype,[isolve(x^2+x*y+4*y^2=p)],negint);</CMP>
