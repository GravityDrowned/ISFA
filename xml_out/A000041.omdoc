      <CMP> def A000041(n):</CMP>
    </omtext><omtext class="program">
      <CMP>     if n == 0: return 1</CMP>
    </omtext><omtext class="program">
      <CMP>     S = 0; J = n-1; k = 2</CMP>
    </omtext><omtext class="program">
      <CMP>     while 0 &lt;= J:</CMP>
    </omtext><omtext class="program">
      <CMP>         T = A000041(J)</CMP>
    </omtext><omtext class="program">
      <CMP>         S = S+T if is_odd(k//2) else S-T</CMP>
    </omtext><omtext class="program">
      <CMP>         J -= k if is_odd(k) else k//2</CMP>
    </omtext><omtext class="program">
      <CMP>         k += 1</CMP>
    </omtext><omtext class="program">
      <CMP>     return S</CMP>
    </omtext><omtext class="program">
