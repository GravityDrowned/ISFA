    </omtext><omtext class="program">
      <CMP> a000050 n = foldl f 0 [1..2^n]</CMP>
    </omtext><omtext class="program">
      <CMP>   where f i j = if a000050' j &gt; 0 then i + 1 else i</CMP>
    </omtext><omtext class="program">
      <CMP> a000050' k = foldl f 0 (h k)</CMP>
    </omtext><omtext class="program">
      <CMP>   where f i y = g y + i</CMP>
    </omtext><omtext class="program">
      <CMP>           where g y = issquare (k - y^2)</CMP>
    </omtext><omtext class="program">
      <CMP>         h k = [0..isqrt k]</CMP>
