      </omdoc:p><omtext class="program">
      <CMP> (Sage) # Algorithm of L. Seidel (1877)</CMP>
    </omtext><omtext class="program">
      <CMP> def A000660_list(n) :</CMP>
    </omtext><omtext class="program">
      <CMP>     R = []; A = {-1:0, 0:1}</CMP>
    </omtext><omtext class="program">
      <CMP>     k = 0; e = 1</CMP>
    </omtext><omtext class="program">
      <CMP>     for i in range(n) :</CMP>
    </omtext><omtext class="program">
      <CMP>         Am = i</CMP>
    </omtext><omtext class="program">
      <CMP>         A[k + e] = 0</CMP>
    </omtext><omtext class="program">
      <CMP>         e = -e</CMP>
    </omtext><omtext class="program">
      <CMP>         for j in (0..i) :</CMP>
    </omtext><omtext class="program">
      <CMP>             Am += A[k]</CMP>
    </omtext><omtext class="program">
      <CMP>             A[k] = Am</CMP>
    </omtext><omtext class="program">
      <CMP>             k += e</CMP>
    </omtext><omtext class="program">
      <CMP>         print [A[z] for z in (-i//2..i//2)]</CMP>
    </omtext><omtext class="program">
      <CMP>         R.append(A[e*i//2])</CMP>
    </omtext><omtext class="program">
      <CMP>     return R</CMP>
    </omtext><omtext class="program">
      <CMP> A000660_list(10) # _Peter Luschny_, Jun 02 2012</CMP>
