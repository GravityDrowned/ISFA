      <CMP> Join[{a=0,b=1},Table[c=2*b-4*a;a=b;b=c,{n,100}]] (* _Vladimir Joseph Stephan Orlovsky_, Jan 17 2011*)</CMP>
    </omtext><omtext class="program">
      <CMP> (Sage) [lucas_number1(n,2,4) for n in xrange(0, 39)] # [From _Zerinvary Lajos_, Apr 23 2009]</CMP>
    </omtext><omtext class="program">
      <CMP> (PARI) /* lists powers of any quaternion) */</CMP>
    </omtext><omtext class="program">
      <CMP> QuaternionToN(a,b,c,d,nmax) = {local (C);C = matrix(nmax+1,4);C[1,1]=1;for(n=2,nmax+1,C[n,1]=a*C[n-1,1]-b*C[n-1,2]-c*C[n-1,3]-d*C[n-1,4];C[n,2]=b*C[n-1,1]+a*C[n-1,2]+d*C[n-1,3]-c*C[n-1,4];C[n,3]=c*C[n-1,1]-d*C[n-1,2]+a*C[n-1,3]+b*C[n-1,4];C[n,4]=d*C[n-1,1]+c*C[n-1,2]-b*C[n-1,3]+a*C[n-1,4];);return (C);} /* Stanislav Sykora, Jun 11 2012 */</CMP>
