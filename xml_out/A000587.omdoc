      <CMP> for _ in range(30):</CMP>
    </omtext><omtext class="program">
      <CMP> ....blist = list(accumulate([b]+blist))</CMP>
    </omtext><omtext class="program">
      <CMP> ....b = -blist[-1]</CMP>
    </omtext><omtext class="program">
      <CMP> ....A000587.append(b) # _Chai Wah Wu_, Sep 19 2014</CMP>
    </omtext><omtext class="program">
      <CMP> (PARI)</CMP>
    </omtext><omtext class="program">
