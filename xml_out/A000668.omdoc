      <CMP> A000668 := proc(n) local i;</CMP>
    </omtext><omtext class="maple">
      <CMP> i := 2^(ithprime(n))-1:</CMP>
    </omtext><omtext class="maple">
      <CMP> if (isprime(i)) then</CMP>
    </omtext><omtext class="maple">
      <CMP>    return i</CMP>
    </omtext><omtext class="maple">
      <CMP> fi: end:</CMP>
