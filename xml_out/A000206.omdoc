      <CMP> with (numtheory):</CMP>
    </omtext><omtext class="maple">
      <CMP> b:= proc(n) option remember;</CMP>
    </omtext><omtext class="maple">
      <CMP>       `if` (n=0, 1, 2^(floor(n/2)-1)</CMP>
    </omtext><omtext class="maple">
      <CMP>            +add (phi(2*d) *2^(n/d), d=divisors(n))/(4*n))</CMP>
    </omtext><omtext class="maple">
      <CMP>     end:</CMP>
    </omtext><omtext class="maple">
      <CMP> a:= n-&gt; `if`(n=0, 1, `if`(irem(n, 2)=0,</CMP>
    </omtext><omtext class="maple">
      <CMP>          (b(2*n) +b(n) +4^(n/2-1) -2^(n/2-1))/2, b(2*n)/2)):</CMP>
