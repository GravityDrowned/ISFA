    </assertion><omtext class="maple">
      <CMP> N:= 100: # to get all terms up to a(N)</CMP>
    </omtext><omtext class="maple">
      <CMP> F:= add(a[i]*q^i,i=0..N) - 1/(1 - add(a[i]*q^(2^i),i=0..ilog2(N))):</CMP>
    </omtext><omtext class="maple">
      <CMP> S:= series(F, q, N+1);</CMP>
    </omtext><omtext class="maple">
      <CMP> sol:= {}:</CMP>
    </omtext><omtext class="maple">
      <CMP> for i from 0 to N do</CMP>
    </omtext><omtext class="maple">
      <CMP>   eq:= subs(sol, coeff(S,q,i));</CMP>
    </omtext><omtext class="maple">
      <CMP>   sol:= sol union {a[i]=solve(eq,a[i])};</CMP>
    </omtext><omtext class="maple">
