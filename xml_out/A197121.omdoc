      <CMP> cr = {}; Do[If[IntegerQ[Sqrt[n]], , kk = NumberFieldFundamentalUnits[Sqrt[n]]; d1 = kk[[1]][[2]][[1]]; d2 = kk[[1]][[1]] kk[[1]][[2]][[2]]; d3 = Expand[(d1 + d2) (d1 - d2)]; If[d3 == 1, k1 = Max[Denominator[d1], Denominator[d2]]; If[k1 == 1, , AppendTo[cr, n]]]], {n, 2, 2000}]; cr</CMP>
