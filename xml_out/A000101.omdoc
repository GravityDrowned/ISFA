      <CMP> s = {3}; gm = 1; Do[p = Prime[n + 1]; g = p - Prime[n]; If[g &gt; gm, Print[p]; AppendTo[s, p]; gm = g], {n, 2, 1000000}]; s  (* _Jean-François Alcover_, Mar 31 2011 *)</CMP>
