      <CMP> A000245 := n -&gt; 3*binomial(2*n, n-1)/(n+2); seq(A000245(n), n=0..27);</CMP>
    </omtext><omtext class="mathematica">
      <CMP> Table[3(2n)!/((n+2)!(n-1)!),{n,0,30}] (* or *) Table[3*Binomial[2n,n-1]/(n+2),{n,0,30}] (* or *) Differences[CatalanNumber[Range[0,31]]] (* _Harvey P. Dale_, Jul 13 2011 *)</CMP>
    </omtext><omtext class="program">
      <CMP> (PARI) a(n)=if(n&lt;1,0,3*(2*n)!/(n+2)!/(n-1)!)</CMP>
    </omtext><omtext class="program">
      <CMP> (Sage) [catalan_number(i+1)-catalan_number(i) for i in xrange(0,28)]# _Zerinvary Lajos_, May 17 2009</CMP>
    </omtext><omtext class="program">
      <CMP> (Sage)</CMP>
    </omtext><omtext class="program">
      <CMP> def A000245_list(n) :</CMP>
    </omtext><omtext class="program">
      <CMP>     D = [0]*(n+1); D[1] = 1</CMP>
    </omtext><omtext class="program">
      <CMP>     b = False; h = 1; R = []</CMP>
    </omtext><omtext class="program">
      <CMP>     for i in range(2*n-1) :</CMP>
    </omtext><omtext class="program">
      <CMP>         if b :</CMP>
    </omtext><omtext class="program">
      <CMP>             for k in range(h,0,-1) : D[k] += D[k-1]</CMP>
    </omtext><omtext class="program">
      <CMP>             h += 1; R.append(D[2])</CMP>
    </omtext><omtext class="program">
      <CMP>         else :</CMP>
    </omtext><omtext class="program">
      <CMP>             for k in range(1,h, 1) : D[k] += D[k+1]</CMP>
    </omtext><omtext class="program">
      <CMP>         b = not b</CMP>
    </omtext><omtext class="program">
      <CMP>     return R</CMP>
    </omtext><omtext class="program">
      <CMP> A000245_list(29) # _Peter Luschny_, Jun 03 2012</CMP>
