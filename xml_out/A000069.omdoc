      <CMP> s := proc(n) local i,j,k,b,sum,ans; ans := [ ]; j := 0; for i while j&lt;n do sum := 0; b := convert(i,base,2); for k to nops(b) do sum := sum+b[ k ]; od; if sum mod 2 = 1 then ans := [ op(ans),i ]; j := j+1; fi; od; RETURN(ans); end; t1 := s(100); A000069 := n-&gt;t1[n]; # s(k) gives first k terms.</CMP>
    </omtext><omtext class="maple">
      <CMP> is_A000069 := n -&gt; type(add(i,i=convert(n,base,2)),odd):</CMP>
