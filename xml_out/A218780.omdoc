      <CMP> (definec (A218780 n) (parenthesization-&gt;A014486 (tree_for_A218780 n)))(definec (tree_for_A218780 n) (cond ((zero? n) (list)) ((= 1 n) (list (list))) (else (let ((new-tree (copy-tree (tree_for_A218780 (-1+ n))))) (add-bud-for-the-n-th-unbranching-tree-with-car-cdr-code! new-tree (A218791 n))))))</CMP>
