    </assertion><omtext class="maple">
      <CMP> s:= proc(n) option remember;</CMP>
    </omtext><omtext class="maple">
      <CMP>        `if` (n=1, add (ithprime(i), i=1..81),</CMP>
    </omtext><omtext class="maple">
      <CMP>                   ithprime(n+80) -ithprime(n-1) +s(n-1))</CMP>
    </omtext><omtext class="maple">
      <CMP>     end:</CMP>
    </omtext><omtext class="maple">
      <CMP> a:= proc(n) option remember; local k, m;</CMP>
    </omtext><omtext class="maple">
      <CMP>        a(n-1);</CMP>
    </omtext><omtext class="maple">
      <CMP>        for k from 1+b(n-1) while irem (s(k), 9, 'm')&lt;&gt;0 do od;</CMP>
    </omtext><omtext class="maple">
      <CMP>        b(n):= k; m</CMP>
    </omtext><omtext class="maple">
      <CMP>     end:</CMP>
    </omtext><omtext class="maple">
      <CMP> a(0):=0: b(0):=0:</CMP>
    </omtext><omtext class="maple">
      <CMP> seq (a(n), n=1..50);</CMP>
