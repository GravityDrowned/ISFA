    </omdoc:p><omtext class="program">
      <CMP> (PARI)</CMP>
    </omtext><omtext class="program">
      <CMP> {</CMP>
    </omtext><omtext class="program">
      <CMP> a=0;d1=0;p=1;print1(&quot;0, 0, 0, &quot;,p,&quot;, &quot;);\\8s2a1</CMP>
    </omtext><omtext class="program">
      <CMP> for(n=2,100,</CMP>
    </omtext><omtext class="program">
      <CMP>    if(n&lt;5,d1=2,</CMP>
    </omtext><omtext class="program">
      <CMP>      if(n&lt;7,d1=3,</CMP>
    </omtext><omtext class="program">
      <CMP>        if(n&lt;8,d1=2,</CMP>
    </omtext><omtext class="program">
      <CMP>          if(Mod(n,4)==0,d1=0,</CMP>
    </omtext><omtext class="program">
      <CMP>            if(Mod(n,4)==1,d1=5,</CMP>
    </omtext><omtext class="program">
      <CMP>              if(Mod(n,4)==2,d1=-1,d1=4</CMP>
    </omtext><omtext class="program">
      <CMP>              )</CMP>
    </omtext><omtext class="program">
      <CMP>            )</CMP>
    </omtext><omtext class="program">
      <CMP>          )</CMP>
    </omtext><omtext class="program">
      <CMP>        )</CMP>
    </omtext><omtext class="program">
      <CMP>      )</CMP>
    </omtext><omtext class="program">
      <CMP>    );</CMP>
    </omtext><omtext class="program">
      <CMP>    a=a+d1;p=p+a;</CMP>
    </omtext><omtext class="program">
      <CMP>    print1(p,&quot;, &quot;)</CMP>
    </omtext><omtext class="program">
      <CMP> )</CMP>
    </omtext><omtext class="program">
      <CMP> }</CMP>
