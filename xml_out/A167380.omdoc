      <CMP> A167380 := proc(n) if n &lt;= 2 then n; else op(1+ (n mod 6),[-4,-5,-1,4,5,1]) ; end if; end proc: seq(A167380(n),n=1..120) ; # R. J. Mathar, Nov 02 3009</CMP>
    </omtext><omtext class="mathematica">
      <CMP> PadRight[{1,2},120,{-5,-1,4,5,1,-4}] (* or *) Join[{1,2},LinearRecurrence[ {1,-1},{4,5},120]] (* _Harvey P. Dale_, Mar 08 2015 *)</CMP>
