        <theory name="A000898">
          <assertion class="***** TTTT *****">
      <CMP> M1648 N0645</CMP>
    </assertion><omtext class="starts-with">
      <CMP> 1,2,6,20,76,312,1384,6512,32400,168992,921184,5222208,30710464,</CMP>
