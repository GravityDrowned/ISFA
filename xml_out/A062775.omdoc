    </OMA><OMA>
      <OMS name="f" cd="A062775"/>
      <OMI>3</OMI><OMV name="j"/>
    </OMA><OMA>
      <OMS name="f" cd="A062775"/>
      <OMI>5</OMI><OMV name="k"/>
    </OMA><OMS name="..." cd="arithmetics"></OMS><OMA>
      <OMS name="f" cd="A062775"/>
      <OMV name="p"/><OMV name="q"/>
    </OMA></OMA>
    </OMA>
      </OMOBJ>where <OMOBJ>
        <OMA>
      <OMS name="=" cd="arithmetic"/>
      <OMA>
      <OMS name="f" cd="A062775"/>
      <OMV name="p"/><OMI>1</OMI>
    </OMA>
      <OMV name="p"/>
    </OMA>
      </OMOBJ>for any prime <OMOBJ>
        <OMV name="p"/>
      </OMOBJ>;<OMOBJ>
        <OMA>
      <OMS name="=" cd="arithmetic"/>
      <OMA>
      <OMS name="f" cd="A062775"/>
      <OMI>2</OMI><OMV name="i"/>
    </OMA>
      <OMA>
      <OMS name="minus" cd="arithmetics"/>
      <OMA>
      <OMS name="plus" cd="arithmetics"/>
      <OMA>
      <OMS name="power" cd="arithmetics"/>
      <OMI>2</OMI>
      <OMV name="i"/>
    </OMA><OMA>
      <OMS name="power" cd="arithmetics"/>
      <OMI>2</OMI>
      <OMV name="i"/>
    </OMA>
    </OMA><OMA>
      <OMS name="power" cd="arithmetics"/>
      <OMI>2</OMI>
      <OMA>
      <OMS name="ceil" cd="A062775"/>
      <OMA>
      <OMS name="divide" cd="arithmetics"/>
      <OMV name="i"/><OMI>2</OMI>
    </OMA>
    </OMA>
    </OMA>
    </OMA>
    </OMA>
      </OMOBJ>;<OMOBJ>
        <OMA>
      <OMS name="=" cd="arithmetic"/>
