      <CMP> a000796 n = a000796_list (n + 1) !! (n + 1)</CMP>
    </omtext><omtext class="program">
      <CMP> a000796_list len = map digitToInt $ show $ machin' `div` (10 ^ 10) where</CMP>
    </omtext><omtext class="program">
      <CMP>    machin' = 4 * (4 * arccot 5 unity - arccot 239 unity)</CMP>
    </omtext><omtext class="program">
      <CMP>    unity = 10 ^ (len + 10)</CMP>
    </omtext><omtext class="program">
      <CMP>    arccot x unity = arccot' x unity 0 (unity `div` x) 1 1 where</CMP>
    </omtext><omtext class="program">
      <CMP>      arccot' x unity summa xpow n sign</CMP>
    </omtext><omtext class="program">
      <CMP>       | term == 0 = summa</CMP>
    </omtext><omtext class="program">
      <CMP>       | otherwise = arccot'</CMP>
    </omtext><omtext class="program">
      <CMP>         x unity (summa + sign * term) (xpow `div` x ^ 2) (n + 2) (- sign)</CMP>
    </omtext><omtext class="program">
      <CMP>       where term = xpow `div` n</CMP>
    </omtext><omtext class="program">
      <CMP> -- _Reinhard Zumkeller_, Nov 24 2012</CMP>
