    </omdoc:p><omtext class="mathematica">
      <CMP> nn = 100; lim = Sqrt[nn]; t = Table[0, {nn}]; Do[n = a^2 + b^2 + c^2 + d^2; If[n &lt;= nn, t[[n]]++], {a, lim}, {b, a, lim}, {c, b, lim}, {d, c, lim}]; t (* _T. D. Noe_, Sep 28 2012 *)</CMP>
    </omtext><omtext class="mathematica">
      <CMP> f[n_] := Length@ IntegerPartitions[n, {4}, Range[ Floor[ Sqrt[n - 1]]]^2]; Array[f, 105] (* _Robert G. Wilson v_, Sep 28 2012 *)</CMP>
    </omtext><omtext class="program">
      <CMP> (PARI) A025428(n)=sum(a=1,n,sum(b=1,a,sum(c=1,b,sum(d=1,c,a^2+b^2+c^2+d^2==n))))</CMP>
    </omtext><omtext class="program">
      <CMP> (PARI) A025428(n)=sum(a=1,sqrtint(max(n-3,0)), sum(b=1,min(sqrtint(n-a^2-2),a), sum(c=1,min(sqrtint(n-a^2-b^2-1),b),issquare(n-a^2-b^2-c^2,&amp;d) &amp; d &lt;= c )))</CMP>
    </omtext><omtext class="program">
      <CMP> (PARI) A025428(n)=sum(a=sqrtint(max(n,4)\4),sqrtint(max(n-3,0)), sum(b=sqrtint((n-a^2)\3-1)+1,min(sqrtint(n-a^2-2),a), sum(c=sqrtint((t=n-a^2-b^2)\2-1)+1, min(sqrtint(t-1),b), issquare(t-c^2) ))) \\ - _M. F. Hasler_, Sep 17 2012</CMP>
    </omtext><omtext class="program">
      <CMP> for(n=1,100,print1(A025428(n),&quot;,&quot;))</CMP>
