      <CMP> a001316_list = 1 : gould [1] where</CMP>
    </omtext><omtext class="program">
      <CMP>    gould (x:xs) = zs ++ gould zs where</CMP>
    </omtext><omtext class="program">
      <CMP>                   zs = (2*x) : xs ++ map (* 2) (x:xs)</CMP>
