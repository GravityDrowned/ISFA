    </omtext><omtext class="program">
      <CMP>     b = True; h = 1; R = []</CMP>
    </omtext><omtext class="program">
      <CMP>     for i in range(2*n-1) :</CMP>
    </omtext><omtext class="program">
      <CMP>         if b :</CMP>
    </omtext><omtext class="program">
      <CMP>             for k in range(h,0,-1) : D[k] += D[k-1]</CMP>
    </omtext><omtext class="program">
